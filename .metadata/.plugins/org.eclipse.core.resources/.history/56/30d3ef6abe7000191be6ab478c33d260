import java.util.Scanner;

public class StuckInATimeLoop {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        for (int i = 0; i <= (t - 1); i++)
            System.out.println((i + 1) + " Abracadabra");
        sc.close();
    }

}
