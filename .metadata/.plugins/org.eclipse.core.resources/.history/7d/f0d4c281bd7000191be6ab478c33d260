import java.util.Scanner;

public class TakeTwoStones {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i = sc.nextInt(), s = 0;
        if (i >= 1 && i <= 10000000)
            s = i % 2;
        if (s == 0)
            System.out.println("Bob");
        else
            System.out.println("Alice");
    }

}
